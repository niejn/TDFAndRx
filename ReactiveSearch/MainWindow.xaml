<Window x:Class="ReactiveSearch.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ReactiveSearch"
        mc:Ignorable="d" FontSize="35"
        Title="Reactive Search" Height="350" Width="525">
    <DockPanel>
        <TextBox x:Name="SearchBox" DockPanel.Dock="Top"></TextBox>
        <ListBox x:Name="SearchResults"></ListBox>
    </DockPanel>
</Window>
